
<div class="container">
  <p id="notice"><%= notice %></p>

  <%= form_with(url: tokenize_path) do %>
    <%= submit_tag("Perform Tokenize Job") %>
  <% end %>
  <p>
    <%= @imported_text.html_content.html_safe %>
  </p>

  <%= link_to 'Edit', edit_imported_text_path(@imported_text) %> |
  <%= link_to 'Back', imported_texts_path %>

</div>
